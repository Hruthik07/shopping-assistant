[flake8]
max-line-length = 100
exclude = 
    .git,
    __pycache__,
    venv,
    .venv,
    build,
    dist,
    *.egg-info,
    .github,
    chroma_db,
    logs,
    evaluation/reports
ignore = 
    E203,
    E501,
    W503,
    E402,
    F401
per-file-ignores =
    __init__.py:F401
max-complexity = 15
